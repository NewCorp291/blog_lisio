<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>

		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
			<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
			<script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.7/dist/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
			<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
		{% endblock %}
	</head>
	<body>
		<nav class="py-2 bg-dark border-bottom">
			<div class="container d-flex felx-wrap">
				<div class="dropdown">
					<a class="nav nav-link dropdown-toggle text-white" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-expanded="false">
						Mon Blog
					</a>
					<ul class="dropdown-menu" aria-labelledby="dropdownMenuButtonAdminDashboard">
						<li>
							<a class="dropdown-item" href="{{ path('admin') }}">Tableau de bord</a>
						</li>
						<li>
							<a class="dropdown-item" href="#">Menu</a>
						</li>
					</ul>
				</div>

				<div class="dropdown">
					<a class="nav nav-link dropdown-toggle text-white" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-expanded="false">
						Ajouter
					</a>
					<ul class="dropdown-menu" aria-labelledby="dropdownMenuButtonAdminAdd">
						<li>
							<a class="dropdown-item" href="#">Article</a>
						</li>
						<li>
							<a class="dropdown-item" href="#">Page</a>
						</li>
					</ul>
				</div>
			</div>
		</nav>

		<section class="container-fluid fond_numerique_dame">
			<div class="container">
				<article class="row">
					<a class="text-dark text-decoration-none" href="{{ path('app_home')}}">
						<h1>Blog</h1>
					</a>
					<p class="mt-3">Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
					<div class="text-center">
						<button type="button" class="btn btn-success mt-1">Nous contacter</button>
					</div>
				</article>
			</div>
		</section>
		<section class="container container-menu">
			<nav class="navbar navbar-expand-lg navbar-light bg-white">
				<div class="container-fluid">
					<button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
						<span class="toggler-icon top-bar"></span>
						<span class="toggler-icon middle-bar"></span>
						<span class="toggler-icon bottom-bar"></span>
					</button>
				</div>
			</nav>
		</section>
		{% block body %}{% endblock %}

		<nav class="d-flex justify-content-center container mb-5" aria-label="...">
			<ul class="pagination">
				<li class="page-item disabled">
					<a class="page-link" href="#" tabindex="-1" aria-disabled="true">Previous</a>
				</li>
				<li class="page-item">
					<a class="page-link" href="#">1</a>
				</li>
				<li class="page-item active" aria-current="page">
					<a class="page-link" href="#">2</a>
				</li>
				<li class="page-item">
					<a class="page-link" href="#">3</a>
				</li>
				<li class="page-item">
					<a class="page-link" href="#">Next</a>
				</li>
			</ul>
		</nav>

		<section class="container-fluid fond_numerique_dame">
			<div class="container">
				<article class="row">
					<p class="mt-footer">Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
					<div class="text-center">
						<button type="button" class="btn btn-success mt-2">Nous contacter</button>
					</div>
				</article>
			</div>
		</section>
	</body>
</html></body></html>
